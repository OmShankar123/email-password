{
  "name": "DuApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "assemble": "cd android && ./gradlew assembleDebug && cd ..",
    "androidRelease": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "removeAndroidDrwable": "rm -rf ./android/app/src/main/res/drawable-*",
    "removeAndroidRaw": "rm -rf ./android/app/src/main/res/raw",
    "AndroidBuild": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "AndroidBundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "gradleClean": "cd android && ./gradlew clean && cd ..",
    "exeAllAndroid": "npm run androidRelease && npm run removeAndroidDrwable && npm run removeAndroidRaw && npm run AndroidBuild && npm run AndroidBundleRelease",
    "java11": "export JAVA_HOME=$(/usr/libexec/java_home -v 11)"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-firebase/app": "^20.5.0",
    "@react-native-firebase/auth": "^20.5.0",
    "@react-native-firebase/firestore": "^20.5.0",
    "@react-native-firebase/storage": "^20.5.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "add": "^2.0.6",
    "axios": "^1.6.4",
    "react": "18.2.0",
    "react-native": "^0.73.9",
    "react-native-axios": "^0.17.1",
    "react-native-blob-util": "^0.19.6",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-permissions": "^4.1.5",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-webview": "^13.6.4",
    "rn-fetch-blob": "^0.12.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
